    "report": "nyc report --reporter=html",
    "posttest": "cross-env NODE_ENV=test node test/posttest.js",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluebrid/koa-learning.git"

  },
  "homepage": "https://github.com/bluebrid/koa-learning#readme",
  "dependencies": {
    "cross-env": "^5.2.0",
    "delegate": "^3.2.0",
    "delegates": "^1.0.0",
    "methods": "^1.1.2",
    "moment": "^2.22.2",
    "nodemon": "^1.18.3",
    "only": "0.0.2",
    "path-to-regexp": "^2.2.1",
    "statuses": "^1.5.0"
